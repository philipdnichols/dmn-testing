    <dmn:itemComponent id="_applicant_creditScore" name="creditScore" isCollection="false">
      <dmn:typeRef>number</dmn:typeRef>
    </dmn:itemComponent>
  </dmn:itemDefinition>
  <dmn:itemDefinition id="_loan" name="Loan" isCollection="false">
